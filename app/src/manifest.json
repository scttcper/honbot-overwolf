{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":"IO Plugin Sample",
    "version":"1.0.0.5",
    "minimum-overwolf-version": "0.77.4.0",
    "author":"Overwolf; Thorwe",
    "icon":"icon.png",
    "icon_gray":"icon_gray.png",
    "description":"Honbot ingame stats for Heroes of Newerth (HoN)",
    "dock_button_title":"honbot"
